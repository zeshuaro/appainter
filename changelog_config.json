{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["enhancement", "fluttersdk"]
    },
    {
      "title": "## ğŸ› Fixes",
      "labels": ["bug"]
    },
    {
      "title": "## ğŸ§ª Tests",
      "labels": ["test"]
    },
    {
      "title": "## ğŸ“„ Documentation",
      "labels": ["documentation"]
    },
    {
      "title": "## ğŸ’¬ Other",
      "labels": ["refactor", "dependencies"]
    }
  ],
  "ignore_labels": ["ignore", "ci", "style"],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}} ([#${{NUMBER}}](${{URL}}))",
  "empty_template": "- no changes",
  "label_extractor": [
    {
      "pattern": "(.) (.+)",
      "target": "$1",
      "flags": "gu"
    },
    {
      "pattern": "\\[Issue\\]",
      "on_property": "title",
      "method": "match"
    }
  ],
  "duplicate_filter": {
    "pattern": "\\[ABC-....\\]",
    "on_property": "title",
    "method": "match"
  },
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
      "target": "- $4\n  - $6"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [],
  "tag_resolver": {
    "method": "semver"
  },
  "base_branches": []
}
